# MSW Mock Mode Environment Configuration
# This mode uses frontend mocks instead of LocalStack/backend

# Disable LocalStack for mock mode
NODE_ENV=development
USE_LOCALSTACK=false

# Frontend Mock Configuration
VITE_API_URL=http://localhost:3001
VITE_APP_ENV=development
VITE_ENABLE_MSW=true
VITE_LOG_LEVEL=debug

# Backend not used in mock mode
# BACKEND_API_URL=http://localhost:3001

# JWT Configuration (not used in mocks but required for env validation)
JWT_SECRET=mock-jwt-secret
JWT_REFRESH_SECRET=mock-jwt-refresh-secret